  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch PrimeSearch",
      "request": "launch",
      "mainClass": "linda.prime.PrimeSearch",
      "projectName": "app",
      "args": "100"
    },
    {
      "type": "java",
      "name": "Launch improved TextSearch",
      "request": "launch",
      "mainClass": "linda.search.improved.TextSearch",
      "projectName": "app",
      "args": ["\"agneau\"", "./app/src/main/resources/dict/french"]
    },
    {
      "type": "java",
      "name": "Launch basic TextSearch",
      "request": "launch",
      "mainClass": "linda.search.basic.TextSearch",
      "projectName": "app",
      "args": ["\"agneau\"", "./app/src/main/resources/dict/french"]
    },
    {
      "type": "java",
      "name": "Launch Whiteboard",
      "request": "launch",
      "mainClass": "linda.whiteboard.Whiteboard",
      "projectName": "app"
    }
  ]
}
